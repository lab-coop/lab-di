{
  "name": "lab-di",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-watch",
    "test:all": "./node_modules/cucumber/bin/cucumber.js ./features --compiler es6:babel-core/register --require ./features ",
    "compile": "babel ./ -d ./ --ignore node_modules,assets,features",
    "prepublish": "npm run compile"
  },
  "watch": {
    "test:all": {
      "quiet": true,
      "patterns": [
        "**"
      ],
      "extensions": [
        "js",
        "json",
        "feature"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bottlejs": "1.5.0",
    "chai": "3.5.0",
    "lab-config": "1.0.0",
    "lab-test": "0.0.3",
    "lodash": "^4.17.10",
    "verror": "1.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-preset-es2015": "6.24.0",
    "cucumber": "1.3.2",
    "npm-watch": "0.1.6"
  }
}
